# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
DMITRY_MODEL=google/gemini-2.0-flash-001

# Platform Integration (Unified API)
PLATFORM_API_URL=http://localhost:9000
PLATFORM_API_KEY=your_platform_api_key_here
PLATFORM_TIMEOUT=30

# Security Integrations (Optional)
SPLUNK_API_KEY=your_splunk_api_key
SPLUNK_API_URL=https://your-splunk-instance:8089

ELASTIC_API_KEY=your_elastic_api_key
ELASTIC_API_URL=https://your-elastic-instance:9200

VIRUSTOTAL_API_KEY=your_virustotal_api_key
MISP_API_KEY=your_misp_api_key
MISP_URL=https://your-misp-instance

# Threat Intelligence
OTX_API_KEY=your_otx_api_key

# Vulnerability Scanners
NESSUS_API_KEY=your_nessus_api_key
NESSUS_URL=https://your-nessus-instance:8834

OPENVAS_API_KEY=your_openvas_api_key
OPENVAS_URL=https://your-openvas-instance

QUALYS_API_KEY=your_qualys_api_key
QUALYS_URL=https://qualysapi.qualys.com

# Cloud Security (Optional)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1

AZURE_TENANT_ID=your_azure_tenant_id
AZURE_CLIENT_ID=your_azure_client_id
AZURE_CLIENT_SECRET=your_azure_client_secret

GCP_PROJECT_ID=your_gcp_project_id
GCP_CREDENTIALS_PATH=/path/to/gcp-credentials.json

# Authentication & Security
JWT_SECRET_KEY=generate_a_random_secret_key_minimum_32_characters
API_RATE_LIMIT=100
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8765

# Logging & Monitoring
LOG_LEVEL=INFO
AUDIT_LOG_PATH=logs/audit.log
METRICS_ENABLED=true
PROMETHEUS_PORT=9090
